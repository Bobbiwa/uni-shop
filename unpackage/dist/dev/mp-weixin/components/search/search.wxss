/* 水平间距 */
.search-container {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  height: 100rpx;
  background-color: #c00000;
}
.search-container .my-search-box {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 72rpx;
  width: 100%;
  background-color: #fff;
  border-radius: 36rpx;
}
.search-container .my-search-box .placeholder {
  font-size: 30rpx;
  margin-left: 10rpx;
}

